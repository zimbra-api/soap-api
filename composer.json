{
    "name": "zelrone/soap-api",
    "type": "library",
    "description": "PHP wrapper library around the Zimbra Soap API (web service) Upgraded to work on Guzzel 7.*",
    "keywords": [
        "library",
        "soap",
        "web service",
        "client",
        "Zimbra API"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Zelrone",
            "email": "zelsdiary@gmail.com",
            "homepage": "https://github.com/Zelrone"
        }
    ],
    "require": {
        "php": "^7.2.5",
        "evenement/evenement": "~2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "phpcollection/phpcollection": "0.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8",
        "fzaninotto/faker": "~v1.5.0"
    },
    "autoload": {
        "psr-4": {
            "Zimbra\\": "src/Zimbra"
        },
        "exclude-from-classmap": [
            "**/Tests/"
        ]
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}