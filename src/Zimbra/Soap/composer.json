{
    "name": "zimbra-api/soap",
    "homepage": "https://github.com/zimbra-api",
    "description": "Soap libraries used by Zimbra Api",
    "keywords": [
        "soap"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Viv",
            "email": "zelsdiary@gmail.com",
            "homepage": "https://github.com/Zelphire/"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "zimbra-api/struct": "self.version"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "fzaninotto/faker": "~v1.5.0"
